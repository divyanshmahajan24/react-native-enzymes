{
  "short_name": "Navigation Tabs Test",
  "name": "Navigation Tabs",
  "description": "This chrome extension will help users to navigate quickly between their tabs.",
  "version": "1.0.0",
  "permissions": ["tabs"],
  "manifest_version": 2,
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
